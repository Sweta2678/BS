}
input.searchBtn[type="submit"] {
  background: rgba(0, 0, 0, 0) url("../images/sprite.png") no-repeat scroll -4px 0;
  border: 0 none;
  cursor: pointer;
  display: inline-block;
  float: right;
  margin: 3px 5px 0 0;
  padding: 0 5px;
  position: relative;
  text-indent: -9999px;
  width: 17px;
}
.wrapper-dropdown-3 {
  color: #fff;
  cursor: pointer;
  font-size: 14.94px;
  font-weight: 800;
  margin: 0 auto;
  outline: medium none;
  padding: 10px;
  position: relative;
  width: auto;
}
.wrapper-dropdown-3 a{color:#fff; text-decoration:none;}
.wrapper-dropdown-3 > a{ margin-right: 30px;}
.wrapper-dropdown-3 a::selection {background: transparent; -ms-user-select: none}
.wrapper-dropdown-3::after {
  background: rgba(0, 0, 0, 0) url("../images/sprite.png") no-repeat scroll -24px 0;
  border-color: #d96932 transparent;
  border-style: solid;
  content: "";
  height: 18px;
  margin-top: -12px;
  position: absolute;
  right: 15px;
  top: 49%;
  width: 14px;
}
.wrapper-dropdown-3 .dropdown {
	width: 200px;
  background: white none repeat scroll 0 0;
  border: 1px solid rgba(0, 0, 0, 0.17);
  border-radius: inherit;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
  display: none;
  font-weight: normal;
  left: auto;
  list-style: outside none none;
  opacity: 0;
  pointer-events: cursor;
  position: absolute;
  right: 0;
  top: 114%;
  transition: all 0.5s ease-in 0s;
}
.wrapper-dropdown-3 .dropdown:after {
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    bottom: 100%;
    right: 15px;
    border-width: 0 6px 6px 6px;
    border-style: solid;
    border-color: #fff transparent;    
}
.wrapper-dropdown-3 .dropdown:before {
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    bottom: 100%;
    right: 13px;
    border-width: 0 8px 8px 8px;
    border-style: solid;
    border-color: rgba(0,0,0,0.1) transparent;    
}
.wrapper-dropdown-3 .dropdown li a {
  border-bottom: 1px solid #e6e8ea;
  color: #000000;
  display: block;
  font-size: 14px;
  font-weight: 500;
  padding: 10px;
  text-decoration: none;
}
.wrapper-dropdown-3 .dropdown li i { float: right; color: inherit;}
.wrapper-dropdown-3 .dropdown li:last-of-type a { border: none;}
/* Hover state */
.wrapper-dropdown-3 .dropdown li:hover a { background: #eeeeee;}
/* Active state */
.wrapper-dropdown-3.active .dropdown { opacity: 1; display:block; z-index: 10000;}
