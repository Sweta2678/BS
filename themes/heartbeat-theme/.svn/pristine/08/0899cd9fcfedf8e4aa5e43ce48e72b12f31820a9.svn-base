.search-container {background: #f2f2f2 none repeat scroll 0 0; margin: 0 auto; width: 701px; padding:20px 20px; overflow:hidden;}
.searcmodify {float: left; width: 625px;}
#searchform {
  margin: 0 auto;
  width: 100%;
}
.searcmodify .keyword.search {
    border: 1px solid #c1bfc0;
    border-radius: 0;
    color: #a7a6a6;
    font-family: calibri;
    font-size: 14px;
    padding: 8px 20px;
    width: 581px;
}
.searchbtn {
    float: left;
    width: 75px;
}
.srcmodify-con .postsearch {
    height: 35px;
    padding: 0 16px;
}
.postsearch {
    background: #428bca none repeat scroll 0 0;
    border: 0 none;
    color: #fff;
    font-size: 14px;
    line-height: 0.8;
    margin: 0 0 0 -2px;
    outline: medium none;
    padding: 14px;
}
.srcmodify-con .postsearch {
    height: 36px;
    padding: 0 16px;
}
.library_result {
    margin: 0 auto;
    overflow: hidden;
    padding: 20px 20px;
    width: 701px;
}
.result_count {
    float: left;
    margin: 7px 0;
    width: 78%;
}
.list_view {
    float: left;
    width: 22%;
}
.search_result .search_list {
    display: inline-block;
    list-style: outside none none;
    margin: 0;
    padding: 0;
}
.search_result ul li {
    border-bottom: 1px solid #ccc;
    display: inline-block;
    padding-bottom: 25px;
    padding-left: 0 !important;
    padding-right: 0;
    padding-top: 25px;
}
.search_result ul li {
    width: 100%;
}
.search_result .search_left {
    float: left;
    width: 10%;
}
.search_result .search_left h6 {
    color: #949293;
    font-size: 10px;
    margin-top: 4px;
    text-align: center;
    text-transform: uppercase;
}
.search_result .search_right {
    border-left: 1px solid #d5d3d4;
    float: right;
    padding-left: 20px;
    width: 84%;
}
.search_result .search_right h3 {
    color: #eb4014;
    font: bold 14px calibri;
    margin-top: 0;
}
.search_result .search_right p {
    color: #333;
    font: 13px Arial,Helvetica,sans-serif;
    margin: 5px 0 16px;
}
.search_result .search_list p {
    margin-bottom: 2px !important;
}
.changeNetworkHeader p {
    line-height: 1;
    margin-bottom: 20px;
}
.resultterm {
    color: #eb4014;
}
.search_result ul li:last-child{border:0px;}
