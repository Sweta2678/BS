.ui-menu .ui-menu-item{padding: 5px 10px}
.ui-menu-item.ui-state-focus{background:none;background-color:#464646;border:0;margin:0;color:#fff}
.ui-selectmenu-open{z-index:105}
.select-container .sel-lable{ float: left;margin-top:6px;margin-right:10px}
