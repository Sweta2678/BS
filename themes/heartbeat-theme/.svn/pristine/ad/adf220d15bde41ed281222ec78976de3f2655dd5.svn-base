/**************carousel ***************/
.jcarousel-wrapper {
  height: 235px;
  margin: 0px auto 50px auto;
  position: relative;
  width: 566px;
}
/** Carousel **/
.jcarousel {position: relative; overflow: hidden;}
.jcarousel ul {
    width: 20000em;
    position: relative;
    list-style: none;
    margin: 0;
    padding: 0;
}
.jcarousel li {float: left;}
.jcarousel-control-prev,
.jcarousel-control-next {
    position: absolute;
    top: 111px;
    width: 42px;
    height: 42px;
    text-align: center;
    background: #da6926;
    color: #fff;
    text-decoration: none;
    text-shadow: 0 0 1px #000;
    font: 24px/27px Arial, sans-serif;
    -webkit-box-shadow: 0 0 2px #999;
    -moz-box-shadow: 0 0 2px #999;
    box-shadow: 0 0 2px #999;
	font-size:40px;
	line-height: 0.8;
	border-radius: 10px;
}
.jcarousel-control-prev, .jcarousel-control-next:hover{color:#fff; text-decoration:none;}
.jcarousel-control-prev, .jcarousel-control-next:focus{color:#fff; text-decoration:none;}
.jcarousel-control-prev {left: 20px;}
.jcarousel-control-prev:focus{text-decoration:none; color: #fff;}
.jcarousel-control-prev:hover{text-decoration:none; color: #fff;}
.jcarousel-control-next {font-size: 42px; padding-top: 0; right: 20px;}
.jcarousel-control-prev:hover span, .jcarousel-control-next:hover span {display: block;}
.jcarousel-control-prev.inactive,.jcarousel-control-next.inactive {cursor: pointer;}
.jcarousel-pagination { margin-top: 25px;text-align:center}
.jcarousel-pagination a {
  background: #b7b7b7 none repeat scroll 0 0;
  border-radius: 14px;
  display: inline-block;
  line-height: 5px;
  margin-right: 7px;
  min-width: 5px;
  padding: 3px;
  text-indent: -9999px;
}
.jcarousel-pagination a.active {
  background: #e88824 none repeat scroll 0 0;
  border: 2px solid #e88824;
  color: #fff;
  opacity: 1;
  text-indent: -9999px;
  padding:1px;
}
.jcarousel-control-prev.inactive, .jcarousel-control-next.inactive{
	background: #818181;
	cursor: default;
	opacity: 0.9;
	filter: alpha(opacity=90) ;
}
.jcarousel .imgcontainer {width:566px;height:271px;position:relative;float:left}
.jcarousel .imgcontainer img{width:566px;height:271px;}
.jcarousel .textcontainer {
	position:absolute;
	bottom:0;
	left:0;
	width:546px;
	height:70px; 
	background-color:#000;
	opacity:.7;
	filter: alpha(opacity=70) ;
	padding:10px;
	color:#fff
	}
.jcarousel .textcontainer h2{padding:0;margin:0;font-size:20px;font-style:italic;font-weight:bold}	
.jcarousel .textcontainer h2 a{color:#fff;}
.jcarousel .textcontainer p{padding:0;margin:0 0 8px 0}
.carousel-container{
	background-color:#f3f4f4;
	background: -webkit-linear-gradient(left, #f3f4f4, #fcfcfc, #f3f4f4); /* For Safari 5.1 to 6.0 */
