package com.ihg.grs.bean;

import java.util.Date;

/**
 * A POJO for documents details for asset library portlet.
 * @author nakul.patel
 *
 */
public class AssetLibraryBean
{

	private String heading;
	private String description;
	private String type;
	private String downloadLink;
	private String imageURL;
	private String extension;
	private long documentId;
	private Date modifiedDate;
	private Date recentAddDate;
	private boolean documentStatus;
	private boolean downloadStatus;
	private String category;
	private String modDate;
	private boolean confidential;
	private boolean categories;
	private String createdDate;
	
	/**
	 * Returns heading of document.
	 * @return String Heading of document.
	 */
	public String getHeading()
	{
		return heading;
	}
	
	/**
	 * Sets heading of document.
	 * @param heading Heading of document.
	 */
	public void setHeading(String heading)
	{
		this.heading = heading;
	}
	
	/**
	 * Returns description of document.
	 * @return String. Description of document.
	 */
	public String getDescription()
	{
		return description;
	}
	
	/**
	 * Sets description of document.
	 * @param description Description of document.
	 */
	public void setDescription(String description)
	{
		this.description = description;
	}
	
	/**
	 * Retrunrs type of document.
	 * @return Type of document.
	 */
	public String getType()
	{
		return type;
	}
	
	/**
	 * Sets type of document.
	 * @param type Type of document.
	 */
	public void setType(String type)
	{
		this.type = type;
	}
	
	/**
	 * Returns download link of the document.
	 * @return String download link.
	 */
	public String getDownloadLink()
	{
		return downloadLink;
	}
	
	/**
	 * Sets download link of document.
	 * @param downloadLink Download link of document.
	 */
	public void setDownloadLink(String downloadLink)
	{
		this.downloadLink = downloadLink;
	}
	
	/**
	 * Returns image of document.
	 * @return String imageURL.
	 */
	public String getImageURL()
	{
		return imageURL;
	}
	
	/**
	 * Sets image url of document.
	 * @param imageURL Image url of document.
	 */
	public void setImageURL(String imageURL)
	{
		this.imageURL = imageURL;
	}
	
	/**
	 * Returns extension of document.
	 * @return String extension.
	 */
	public String getExtension()
	{
		return extension;
	}
	
	/**
	 * Sets extension of document.
	 * @param extension Extension of document.
	 */
	public void setExtension(String extension)
	{
		this.extension = extension;
	}
	
	/**
	 * Returns id of document.
	 * @return long documentId.
	 */
	public long getDocumentId()
	{
		return documentId;
	}
	
	/**
	 * Sets document id.
	 * @param documentId Id of document.
	 */
	public void setDocumentId(long documentId)
	{
		this.documentId = documentId;
	}
	
	/**
	 * Returns last modified date of document. 
	 * @return Date modifiedDate last modified date.
	 */
	public Date getModifiedDate()
	{
		return modifiedDate;
	}
	
	/**
	 * Sets last modified date of document.
	 * @param modifiedDate Last modified date of document.
	 */
	public void setModifiedDate(Date modifiedDate)
	{
		this.modifiedDate = modifiedDate;
	}
	
	/**
	 * Returns recently added documents date.
	 * @return Date recentAddDate recently added.
	 */
	public Date getRecentAddDate()
	{
		return recentAddDate;
	}
	
	/**
	 * Sets recently added date.
	 * @param recentAddDate Recently added date of document.
	 */
	public void setRecentAddDate(Date recentAddDate)
	{
		this.recentAddDate = recentAddDate;
	}
	
	/**
	 * Returns sataus of the document.
	 * @return Boolean status.
	 */
	public boolean isDocumentStatus()
	{
		return documentStatus;
	}
	
	/**
	 * Sets status of the document.
	 * @param documentStatus Status of document.
	 */
	public void setDocumentStatus(boolean documentStatus)
	{
		this.documentStatus = documentStatus;
	}
	
	/**
	 * Returns download status of document.
	 * @return boolean download status.
	 */
	public boolean isDownloadStatus()
	{
		return downloadStatus;
	}
	
	/**
	 * Sets download status of document.
	 * @param downloadStatus Download status of document.
	 */
	public void setDownloadStatus(boolean downloadStatus)
	{
		this.downloadStatus = downloadStatus;
	}
	
	/**
	 * Returns the category of document.
	 * @return String category.
	 */
	public String getCategory()
	{
		return category;
	}
	
	/**
	 * Sets the category of document.
	 * @param category Category of document.
	 */
	public void setCategory(String category)
	{
		this.category = category;
	}
	
	/**
	 * Returns modified date of document.
	 * @return String modified date.
	 */
	public String getModDate()
	{
		return modDate;
	}
	
	/**
	 * Sets modified date of document.
	 * @param modDate Modified date of document.
	 */
	public void setModDate(String modDate)
	{
		this.modDate = modDate;
	}
	
	/**
	 * Retruns if the document is confidential.
	 * @return boolean confidential. 
	 */
	public boolean isConfidential()
	{
		return confidential;
	}
	
	/**
	 * Sets if the document is confidential.
	 * @param confidential If the document is confidential or not.
	 */
	public void setConfidential(boolean confidential)
	{
		this.confidential = confidential;
	}
	
	/**
	 * Returns if the document has category.
	 * @return boolean categories 
	 */
	public boolean isCategories()
	{
		return categories;
	}
	
	/**
	 * Sets category to document.
	 * @param categories Category of document.
	 */
	public void setCategories(boolean categories)
	{
		this.categories = categories;
	}

	/**
	 * Returns created date of document.
	 * @return String created date.
	 */
	public String getCreatedDate()
	{
		return createdDate;
	}

	/**
	 * Sets created date of document.
	 * @param createdDate Created date of document.
	 */
	public void setCreatedDate(String createdDate)
	{
		this.createdDate = createdDate;
	}
	
	
}
