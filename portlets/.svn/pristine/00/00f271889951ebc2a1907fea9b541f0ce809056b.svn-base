package com.ihg.brandstandards.bsadmin.model;

public class StdAttributes {
	private long stdId;
	private String stdType;
	private String stdTitle;
	private String indexTitle;
	private String indexDesc;
	private String technicalWriter;
	private String author;
	private String countries;
	private String flags;
	private String brands;
	private String areaCategory;
	
	public long getStdId() {
		return stdId;
	}
	public void setStdId(long stdId) {
		this.stdId = stdId;
	}
	public String getStdType() {
		return stdType;
	}
	public void setStdType(String stdType) {
		this.stdType = stdType;
	}
	public String getStdTitle() {
		return stdTitle;
	}
	public void setStdTitle(String stdTitle) {
		this.stdTitle = stdTitle;
	}
	public String getIndexTitle() {
		return indexTitle;
	}
	public void setIndexTitle(String indexTitle) {
		this.indexTitle = indexTitle;
	}
	public String getIndexDesc() {
		return indexDesc;
	}
	public void setIndexDesc(String indexDesc) {
		this.indexDesc = indexDesc;
	}
	public String getTechnicalWriter() {
		return technicalWriter;
	}
	public void setTechnicalWriter(String technicalWriter) {
		this.technicalWriter = technicalWriter;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	public String getCountries() {
		return countries;
	}
	public void setCountries(String countries) {
		this.countries = countries;
	}
	public String getFlags() {
		return flags;
	}
	public void setFlags(String flags) {
		this.flags = flags;
	}
	public String getBrands() {
		return brands;
	}
	public void setBrands(String brands) {
		this.brands = brands;
	}
	public String getAreaCategory() {
		return areaCategory;
	}
	public void setAreaCategory(String areaCategory) {
		this.areaCategory = areaCategory;
	}
	
	

}
