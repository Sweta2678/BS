<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.ihg.brandstandards.db">
    <author>Mummanedi</author>
    <namespace>bsservices</namespace>
 
    <!-- ################### Common Service Entity Configuration will go here ################### -->
    
    <entity name="UserPreference" cache-enabled="false" table="USR_PREF" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="preferenceId" db-name="PREF_ID" type="long" primary="true" />

		<!--  Regular fields -->
		<column name="userId" db-name="USR_ID" type="long" />
		<column name="langCode" db-name="LOCALE_CD" type="String" />
		<column name="pdfLangCode" db-name="PDF_LOCALE_CD" type="String" />
		<column name="buildType" db-name="BUILD_TYP" type="String" />
		<column name="chainCode" db-name="CHAIN_CD" type="String" />
		<column name="reportType" db-name="RPT_TYP" type="String" />
		<column name="paperSize" db-name="RPT_PAPER_SIZE" type="String" />
		<column name="itemsPerPage" db-name="ITEMS_PER_PAGE" type="String" />
		<column name="infoDisplay" db-name="INFO_DISP" type="String" />
		<column name="CountryCode" db-name="CTRY_NM_CD" type="String" />
		<column name="unsubscribeEmail" db-name="UNSUBSCRIBE_CMT_EML" type="String" />
		<column name="moduleName" db-name="MODULE_NM" type="String" />
		<!-- Must to have fields as per DB Team -->
		<column name="creatorId" db-name="CREAT_USR_ID" type="String" />
		<column name="createdDate" db-name="CREAT_TS" type="Date" />
		<column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
		<column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
		
		<!-- Finder methods -->
		<finder name="preferenceId" return-type="UserPreferences">
			<finder-column name="preferenceId" />
		</finder>
		<finder name="UserIdAndModule" return-type="UserPreferences">
			<finder-column name="userId" />
			<finder-column name="moduleName" />
		</finder>
	</entity>
	
	<!-- UI Element Portlet Tables Start -->
	<entity name="UIElement" table="UI_ELEM" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
		<column name="elementId" db-name="UI_ELEM_ID" type="long" primary="true" />
		<column name="elementKey" db-name="UI_ELEM_KEY" type="String" />
		<column name="moduleName" db-name="TGT_MODULE_NM" type="String" />
		<!-- Must to have fields as per DB Team -->
		<column name="creatorId" db-name="CREAT_USR_ID" type="String" />
		<column name="createdDate" db-name="CREAT_TS" type="Date" />
		<column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
		<column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
		<!-- Order -->
		<order by="asc">
			<order-column name="elementId" />
		</order>
		<!-- Finder methods -->
		<finder name="ElementKey" return-type="UIElement">
			<finder-column name="elementKey" />
		</finder>
		<finder name="ModuleName" return-type="Collection">
			<finder-column name="moduleName" />
		</finder>
		<finder name="KeyAndModuleName" return-type="UIElement">
			<finder-column name="elementKey" />
			<finder-column name="moduleName" />
		</finder>
	</entity>
	<entity name="UIElementTranslate" table="UI_ELEM_XLAT" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
		<column name="elementId" db-name="UI_ELEM_ID" type="long" primary="true" />
		<column name="localeCode"  db-name="LOCALE_CD" type="String" primary="true" />
		<column name="elementValue"  db-name="ELEM_VAL" type="String" />
		<!-- Must to have fields as per DB Team -->
		<column name="creatorId" db-name="CREAT_USR_ID" type="String" />
		<column name="createdDate" db-name="CREAT_TS" type="Date" />
		<column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
		<column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
		<!-- Order -->
		<order by="asc">
			<order-column name="elementId" />
		</order>
		<!-- Finder methods -->
		<finder name="ElementId" return-type="Collection">
			<finder-column name="elementId" />
		</finder>
		<finder name="LocaleCode" return-type="Collection">
			<finder-column name="localeCode" />
		</finder>
		<finder name="IdAndLocaleCode" return-type="UIElementTranslate">
			<finder-column name="elementId" />
			<finder-column name="localeCode" />
		</finder>
	</entity>
	<!-- UI Element Portlet Tables End -->
	
	<entity name="FlagCategory"  table="FLAG_CATGY" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
	    <!-- PK fields -->
		<column name="flagCatId" db-name="FLAG_CATGY_ID" type="long" primary="true"/>
        <column name="activeInd" db-name="ACTV_IND" type="String" primary="true"/>
        
        <!--  Regular fields -->
        <column name="parentCatId" db-name="PRNT_FLAG_CATGY_ID" type="long" />
        <column name="catOrField" db-name="CATGY_CD" type="String" />
        <column name="txtValue" db-name="FLAG_CATGY_VAL" type="String" />
        <column name="orderNumber" db-name="DISP_ORDER_NBR" type="long"/>
        
        <!-- Must to have fields as per DB Team -->     
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />

        <!--Order by Clause  -->
        <order by="asc">
            <order-column name="orderNumber"></order-column>
        </order>
        
        <finder name="parentCatId" return-type="Collection">
            <finder-column name="parentCatId" />
            <finder-column name="activeInd" />
        </finder>
        
        <finder name="catOrField" return-type="Collection">
            <finder-column name="catOrField" />
            <finder-column name="activeInd" />
        </finder>
    </entity>
    
    <entity name="StandardsRegion" table="BRNDSTND_RGN" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="regionId" db-name="RGN_ID" type="long" primary="true"/>
       <!--  Regular fields -->
        <column name="regionCode" db-name="RGN_CD" type="String" />
        <column name="regionType" db-name="RGN_TYP_CD" type="String"></column>
        <column name="regionName" db-name="RGN_NM" type="String"></column>
        <column name="isActive" db-name="ACTV_IND" type="String"></column>
        <!-- Must to have fields as per DB Team -->     
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />        
		<column name="parentRegionId" db-name="PRNT_RGN_ID"	 type="long" />

        <finder name="regionId" return-type="StandardsRegion">
            <finder-column name="regionId" />
        </finder>
        <finder name="RegionCode" return-type="StandardsRegion">
            <finder-column name="regionCode" />
        </finder>
    </entity>

    <entity name="StandardsCountry" table="BRNDSTND_CTRY" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <column name="countryNameCode" db-name="CTRY_NM_CD" type="String" primary="true" />
        <column name="regionId" db-name="RGN_ID" type="long" />
        <!-- Must to have fields as per DB Team -->
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
        <column name="subRegionId" db-name="SUB_RGN_ID" type="long" />
        <!-- Order -->
        <order by="asc">
            <order-column name="regionId" />
            <order-column name="countryNameCode" />
        </order>
        <!-- Finder methods -->
        <finder name="RegionId" return-type="Collection">
            <finder-column name="regionId" />
        </finder>       
    </entity>
    
    <entity name="StandardsLocale" table="BRNDSTND_LOCALE" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="localeCode" db-name="LOCALE_CD" type="String" primary="true"/>
        <!--  Regular fields -->
        <column name="countryCode" db-name="CTRY_CD" type="String" />
        <column name="localeName" db-name="LOCALE_NM" type="String"></column>
        <column name="languageCode" db-name="LANG_CD" type="String"></column>
        <column name="orderNumber" db-name="DISP_ORDER_NBR" type="long"></column>
        <column name="isActive" db-name="ACTV_IND" type="String"></column>
        <!-- Must to have fields as per DB Team -->     
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />        
        
        <order>
            <order-column name="orderNumber" order-by="asc"/>
        </order>
        
        <finder name="localeCode" return-type="StandardsLocale">
            <finder-column name="localeCode" />
        </finder>
        <finder name="ActiveStatus" return-type="Collection">
            <finder-column name="isActive" />
        </finder>
    </entity>
    
    <entity name="StandardsRegionLocale" table="BRNDSTND_RGN_LOCALE" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="regionId" db-name="RGN_ID" type="long" primary="true"/>
        <column name="localeCode" db-name="LOCALE_CD" type="String" primary="true"/>
        <!--  Regular fields -->
        <column name="isDefault" db-name="DFLT_LANG_IND" type="String" />
        <!-- Must to have fields as per DB Team -->     
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />        

        <finder name="regionId" return-type="Collection">
            <finder-column name="regionId" />
        </finder>
        <finder name="regionIdIsDefault" return-type="StandardsRegionLocale">
            <finder-column name="isDefault" />
            <finder-column name="regionId" />
        </finder>
        <finder name="localeCodeRegionId" return-type="StandardsRegionLocale">
            <finder-column name="localeCode" />
            <finder-column name="regionId" />
        </finder>
    </entity>
    
    <entity name="PublishCountryStandard" table="PUBLISH_CTRY_STD" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="publishCountryStdId" db-name="PUBLISH_CTRY_STD_ID" type="String" primary="true"/>
              
        <!--  Regular fields -->
        <column name="publishVerId" db-name="PUBLISH_STD_VER_ID" type="long" />
        <column name="countryCode" db-name="CTRY_NM_CD" type="String" />
        <column name="expiredRefTxt" db-name="EXPIRED_BY_REF_TXT" type="String" />
        <column name="publishStatCode" db-name="PUBLISH_STAT_CD" type="String" />
        <column name="stdComplyDeadlineDate" db-name="STD_CMPLY_DEADLINE_DT" type="Date" />
        <column name="orderNumber" db-name="DISP_ORDER_NBR" type="long" />
                
        <!-- Must to have fields as per DB Team -->
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
            
        <finder name="publishVerIdCountry" return-type="PublishCountryStandard">
            <finder-column name="publishVerId" />
            <finder-column name="countryCode" />
        </finder>
        <finder name="publishCountryStdId" return-type="PublishCountryStandards">
            <finder-column name="publishCountryStdId" />
        </finder>
    </entity>
    
    <entity name="Publication" table="PUBLISH" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="publishId" db-name="PUBLISH_ID" type="long" primary="true" />
        
        <!--  Regular fields -->
        <column name="publishVerId" db-name="PUBLISH_VER_ID" type="long" />
        <column name="publishStatCd" db-name="PUBLISH_STAT_CD" type="String" />
        <column name="chainCode" db-name="CHAIN_CD" type="String" />
        <column name="publishEnvName" db-name="PUBLISH_ENVIRONMENT_NM" type="String" />
        <column name="publishLocaleList" db-name="PUBLISHED_LOCALE_LIST_TXT" type="String" />
        <column name="publishDate" db-name="PUBLISH_DT" type="Date" />
        <column name="publishContentType" db-name="PUBLISH_CONTENT_TYP" type="String" />
        <column name="parentPublishId" db-name="PRNT_PUBLISH_ID" type="long" />
        <column name="validateTranslation" db-name="VLD_XLAT_IND" type="String" />
        
        <!-- Must to have fields as per DB Team -->
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
        
        <finder name="ChainCodeAndStatus" return-type="Collection">
            <finder-column name="chainCode" />
            <finder-column name="publishStatCd" />
        </finder>
        <finder name="PublishStatEnvChn" return-type="Publication">
            <finder-column name="publishStatCd" />
            <finder-column name="publishEnvName" />
            <finder-column name="chainCode" />
        </finder>
        <finder name="PublishEnvChn" return-type="Collection">
            <finder-column name="publishEnvName" />
            <finder-column name="chainCode" />
        </finder>
        <finder name="PublishObject" return-type="Publication">
            <finder-column name="chainCode" />
            <finder-column name="publishEnvName" />
            <finder-column name="publishStatCd" />
        </finder>
        <finder name="PublishHistory" return-type="Collection" where="publishStatCd != 'DELETE'">
            <finder-column name="chainCode" />
            <finder-column name="publishEnvName" />
        </finder>
        <finder name="PublishInActiveHistory" return-type="Collection">
            <finder-column name="chainCode" />
            <finder-column name="publishEnvName" />
            <finder-column name="publishStatCd" />
        </finder>
    </entity>
    
    <entity name="PublishQueue" table="PUBLISH_QUEUE" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="publishQueueId" db-name="PUBLISH_QUEUE_ID" type="long" primary="true" />

        <!--  Regular fields -->
        <column name="publishFileText" db-name="PUBLISH_FILE_TXT" type="String" />
        <column name="publishEnvironment" db-name="PUBLISH_ENVIRONMENT_NM" type="String" />
        <column name="schedulePublish" db-name="SCHED_PUBLISH_TS" type="Date" />
        <column name="queueStatCd" db-name="QUEUE_STAT_CD" type="String" />
        <column name="chainCd" db-name="CHAIN_CD" type="String" />
        <column name="notificationEmailId" db-name="NOTIFICATION_EMAIL_ID" type="String" />
        <column name="scheduleStart" db-name="SCHED_STRT_TS" type="Date" />
        <column name="scheduleEnd" db-name="SCHED_END_TS" type="Date" />        
        <column name="expectedProdPublishDt" db-name="EXPT_PROD_PUBLISH_DT" type="Date" />        
        <column name="publishId" db-name="PUBLISH_ID" type="long" />
        <column name="publishLocaleList" db-name="LOCALE_LIST_TXT" type="String" />
        
        <!-- Must to have fields as per DB Team -->
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
                        
        <order>
            <order-column name="publishQueueId" order-by="desc"/>
        </order>

        <finder name="PublishQueueByEnvironment" return-type="Collection">
            <finder-column name="publishEnvironment" />
        </finder>

        <finder name="PublishQueueId" return-type="PublishQueue">
            <finder-column name="publishQueueId" />
        </finder>

        <finder name="PublishQueueByPublishId" return-type="PublishQueue">
            <finder-column name="publishId" />
        </finder>
    </entity>
    
