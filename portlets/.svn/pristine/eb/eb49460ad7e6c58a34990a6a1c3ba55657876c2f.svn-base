    <!-- Glossary Admin Portlet Table Start -->
    <entity name="Glossary" table="GLOSSARY" local-service="true" remote-service="false" data-source="brandstandDataSource" session-factory="BRNDSTNDSessionFactory">
        <!-- PK fields -->
        <column name="glossaryId" db-name="GLOSSARY_ID" type="long" primary="true" />

        <!--  Regular fields -->
        <column name="glossaryTermText" db-name="GLOSSARY_TERM_TXT" type="String" />
        <column name="glossaryTermDesc" db-name="GLOSSARY_TERM_DESC" type="String" />
        <column name="activeInd" db-name="ACTV_IND" type="String" />
        <column name="glossaryOwner" db-name="OWNER_RGN_ID" type="long" />
        
        <!-- Must to have fields as per DB Team -->
        <column name="creatorId" db-name="CREAT_USR_ID" type="String" />
        <column name="createdDate" db-name="CREAT_TS" type="Date" />
        <column name="updatedBy" db-name="LST_UPDT_USR_ID" type="String" />
        <column name="updatedDate" db-name="LST_UPDT_TS" type="Date" />
        
        <finder name="glossaryId" return-type="Collection">
            <finder-column name="glossaryId" />
            <finder-column name="glossaryTermText" comparator="LIKE" />
        </finder>       
        <finder name="glossaryTermText" return-type="Glossary">
            <finder-column name="glossaryTermText" case-sensitive="false"/>
        </finder>
